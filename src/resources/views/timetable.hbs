<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>My UET</title>
    <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css">
    <link rel="stylesheet" href="/css/timetable.css">
    </style>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-aFq/bzH65dt+w6FI2ooMVUpc+21e0SRygnTpmBvdBgSdnuTN7QbdgL+OapgHtvPp" crossorigin="anonymous">
</head>

<body>
    <header>
        {{>header}}
    </header>

    <div class="container">
        <div class="w-95 w-md-75 w-lg-60 w-xl-55 mx-auto mb-6 text-center">
            <div class="subtitle alt-font"><span class="text-primary"></span><span class="title"></span>
            </div>
            <h2 class="display-18 display-md-16 display-lg-14 mb-0"> <span class="text-primary">Timetable</span></h2>
        </div>
        <div class="row overflow-auto">
            <div class="col-md-12">
                <div class="schedule-table">
                    <table class="table bg-white ">
                        <thead>
                            <tr>
                                <th class="day">Routine</th>
                                <th>7h-7h50</th>
                                <th>8h-8h50</th>
                                <th>9h-9h50</th>
                                <th>10h-10h50</th>
                                <th>11h-11h50</th>
                                <th>12h-12h50</th>
                                <th>13h-13h50</th>
                                <th>14h-14h50</th>
                                <th>15h-15h50</th>
                                <th>16h-16h50</th>
                                <th>17h-17h50</th>
                                <th class="last">18h-18h50</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each array}}

                            <tr>

                                <td class="day">{{this.[0]}}</td>

                                <td class="active">
                                    <h4>{{this.[1].course_name}}</h4>
                                    <p>{{this.[1].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[1].course_name}}</h4>
                                        <p>{{this.[1].room}}</p>
                                        <span>{{this.[1].teacher_name}}</span>
                                    </div>

                                </td>

                                <td class="active">
                                    <h4>{{this.[2].course_name}}</h4>
                                    <p>{{this.[2].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[2].course_name}}</h4>
                                        <p>{{this.[2].room}}</p>
                                        <span>{{this.[2].teacher_name}}</span>
                                    </div>

                                </td>
                                <td class="active">
                                    <h4>{{this.[3].course_name}}</h4>
                                    <p>{{this.[3].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[3].course_name}}</h4>
                                        <p>{{this.[3].room}}</p>
                                        <span>{{this.[3].teacher_name}}</span>
                                    </div>

                                </td>
                                <td class="active">
                                    <h4>{{this.[4].course_name}}</h4>
                                    <p>{{this.[4].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[4].course_name}}</h4>
                                        <p>{{this.[4].room}}</p>
                                        <span>{{this.[4].teacher_name}}</span>
                                    </div>

                                </td>
                                <td class="active">
                                    <h4>{{this.[5].course_name}}</h4>
                                    <p>{{this.[5].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[5].course_name}}</h4>
                                        <p>{{this.[5].room}}</p>
                                        <span>{{this.[5].teacher_name}}</span>
                                    </div>

                                </td>
                                <td class="active">
                                    <h4>{{this.[6].course_name}}</h4>
                                    <p>{{this.[6].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[6].course_name}}</h4>
                                        <p>{{this.[6].room}}</p>
                                        <span>{{this.[6].teacher_name}}</span>
                                    </div>

                                </td>
                                <td class="active">
                                    <h4>{{this.[7].course_name}}</h4>
                                    <p>{{this.[7].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[7].course_name}}</h4>
                                        <p>{{this.[7].room}}</p>
                                        <span>{{this.[7].teacher_name}}</span>
                                    </div>

                                </td>
                                <td class="active">
                                    <h4>{{this.[8].course_name}}</h4>
                                    <p>{{this.[8].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[8].course_name}}</h4>
                                        <p>{{this.[8].room}}</p>
                                        <span>{{this.[8].teacher_name}}</span>
                                    </div>

                                </td>
                                <td class="active">
                                    <h4>{{this.[9].course_name}}</h4>
                                    <p>{{this.[9].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[9].course_name}}</h4>
                                        <p>{{this.[9].room}}</p>
                                        <span>{{this.[9].teacher_name}}</span>
                                    </div>

                                </td>
                                <td class="active">
                                    <h4>{{this.[10].course_name}}</h4>
                                    <p>{{this.[10].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[10].course_name}}</h4>
                                        <p>{{this.[10].room}}</p>
                                        <span>{{this.[10].teacher_name}}</span>
                                    </div>

                                </td>
                                <td class="active">
                                    <h4>{{this.[11].course_name}}</h4>
                                    <p>{{this.[11].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[11].course_name}}</h4>
                                        <p>{{this.[11].room}}</p>
                                        <span>{{this.[11].teacher_name}}</span>
                                    </div>

                                </td>
                                <td class="active">
                                    <h4>{{this.[12].course_name}}</h4>
                                    <p>{{this.[12].room}}</p>
                                    <div class="hover">
                                        <h4>{{this.[12].course_name}}</h4>
                                        <p>{{this.[12].room}}</p>
                                        <span>{{this.[12].teacher_name}}</span>
                                    </div>

                                </td>



                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-qKXV1j0HvMUeCBQ+QVp7JcfGl760yU08IQ+GpUo5hlbpg51QRiuqHAJz8+BrxE/N"
        crossorigin="anonymous"></script>
    <script>
    </script>
    <script src="ajax.googleapis.com/ajax/libs/jquery/2.1.3/…"></script>
    <script src="maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/…"></script>
</body>

</html>